{
  "name": "mirza-tahir-ahmad-question-answer-api",
  "version": "0.0.1",
  "description": "An API which provides access to hundreds of Questions and Answers by Hazrat Mirza Tahir Ahmad.",
  "main": "app.js",
  "scripts": {
    "buildDocs": "apidoc -i src/api -o src/static_files/docs -t src/templates/apidocs_template/template/ --private true",
    "dev": "npm run buildDocs; nodemon ./src/app.js",
    "debug": "npm run buildDocs; nodemon --inspect-brk ./src/app.js",
    "start": "npm run buildDocs; node --trace-warnings ./src/app.js --name mirza-tahir-ahmad-question-answer-api",
    "test": "mocha --timeout 15000 src/**/*.test.js --exit"
  },
  "author": "Hashir Ahmad",
  "license": "ISC",
  "dependencies": {
    "apidoc": "^0.17.7",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsdom": "^21.1.1",
    "moment": "^2.29.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "@serverless/cloud": "^2.0.1",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-webpack-aliases": "^1.1.3",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-literal-blacklist": "^0.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sql": "^1.1.1",
    "esm": "^3.2.25",
    "mocha": "^5.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.0",
    "prettier-eslint": "^12.0.0",
    "sinon": "^9.2.4"
  },
  "engines": {
    "node": "^14.6.0"
  }
}
